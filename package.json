{
  "name": "js-streaming",
  "version": "0.1.0",
  "description": "Unified streaming client for WebSocket, SSE, HTTP streaming, long-polling, HLS, and WebRTC.",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/hooks/vue.d.ts",
      "import": "./dist/hooks/vue.mjs",
      "require": "./dist/hooks/vue.cjs"
    },
    "./react": {
      "types": "./dist/hooks/react.d.ts",
      "import": "./dist/hooks/react.mjs",
      "require": "./dist/hooks/react.cjs"
    }
  },
  "scripts": {
    "build": "tsup && node copy-public.js",
    "dev": "tsup --watch",
    "demo": "npm run build && npx http-server -p 8080 -c-1 ."
  },
  "peerDependencies": {
    "hls.js": "^1.5.16",
    "react": ">=17 || >=18",
    "vue": ">=3.3"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "http-server": "^14.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "stream",
    "websocket",
    "sse",
    "hls",
    "webrtc",
    "vue",
    "react"
  ]
}